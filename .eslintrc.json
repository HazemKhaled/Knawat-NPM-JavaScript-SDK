{
  "root": true,
  "env": {
    "es6": true,
    "node": true,
    "commonjs": true,
    "jest": true
  },
  "plugins": ["babel", "prettier"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error"],
    "babel/semi": 1
  }
}
